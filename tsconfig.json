{
  "extends": "@tsconfig/next-strictest/tsconfig.json",
  "compilerOptions": {
    "target": "es2015",
    "module": "es2020",
    "sourceMap": true,
    "noEmit": false,
    "noUnusedLocals": false,
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "downlevelIteration": true,
    "jsx": "react",
    "jsxFactory": "DOMcreateElement",
    "lib": ["ES2020", "DOM", "DOM.Iterable"]
  },
  "include": ["src", "config", "bin"],
}

